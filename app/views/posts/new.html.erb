<h1>new posts</h1>
<%= form_for @post do |f| %>
<p>
  <%= f.label :author, class: 'form-label'%>:
  <%= f.text_field :author %>
</p>
<p>
  <%= f.label :title, class: 'form-label' %>:
  <%= f.text_field :title %><br/>
</p>
<p>
  <%= f.label :body, class: 'form-label' %>:
  <%= f.text_field :body %><br/>
</p>
  <%= f.submit %>
<% end %>
